{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "post",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "pubDate": {
      "type": "datetime"
    },
    "guid": {
      "type": "uid"
    },
    "link": {
      "type": "string"
    },
    "title": {
      "type": "string"
    },
    "description": {
      "type": "richtext",
      "required": false
    },
    "slug": {
      "type": "string",
      "required": true,
      "default": "/YYYY/MM/slug"
    },
    "slugg": {
      "type": "customField",
      "customField": "plugin::slugger.slugger"
    }
  }
}
